@model IEnumerable<OS.Sys.Dominio.Entidades.Ordem_de_servico>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Administrativo/Views/Shared/_Administrativo_layout.cshtml";
}
<br />
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.num_os)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.nome_cliente_os)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.status_os)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.equipamento_os)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.data_de_criacao)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.num_os)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.nome_cliente_os)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.status_os)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.equipamento_os)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.data_de_criacao)
        </td>
        
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id_os }) |
            @Html.ActionLink("Details", "Details", new { id=item.id_os }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id_os })
        </td>
    </tr>
}

</table>
